{
  "name": "@jokeyrhyme/node-init",
  "version": "3.1.0",
  "description": "impose my will upon a new or existing Node.js project",
  "keywords": [],
  "homepage": "https://github.com/jokeyrhyme/node-init.js#readme",
  "bugs": {
    "url": "https://github.com/jokeyrhyme/node-init.js/issues"
  },
  "license": "BSD-2-Clause",
  "author": "Ron Waldon <jokeyrhyme@gmail.com> (https://github.com/jokeyrhyme)",
  "main": "lib/index.js",
  "bin": {
    "node-init": "bin/index.js"
  },
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jokeyrhyme/node-init.js.git"
  },
  "scripts": {
    "eslint": "eslint --fix --cache .",
    "flow": "npx -q flow-bin@^0.64.0 check",
    "jest": "jest",
    "nsp": "npx -q nsp check",
    "sort-package-json": "npx -q sort-package-json",
    "pretest": "npm run prettier && npm run eslint && npm run sort-package-json",
    "test": "npm run jest && npm run nsp && npm run flow",
    "prettier": "npx -q prettier --list-different --write \"**/*.{css,js,jsx,less,scss,ts,tsx}\" || exit 0"
  },
  "dependencies": {
    "chalk": "2.3.2",
    "detect-flowtype": "2.0.0",
    "execa": "0.10.0",
    "figures": "2.0.0",
    "graceful-fs": "4.1.11",
    "has-yarn": "1.0.0",
    "idempotent-fs": "1.2.0",
    "js-yaml": "3.11.0",
    "latest-version": "^3.1.0",
    "load-json-file": "4.0.0",
    "locate-path": "^2.0.0",
    "lodash.intersection": "4.4.0",
    "lodash.union": "^4.5.0",
    "lodash.without": "^4.3.0",
    "log-update": "2.3.0",
    "meow": "4.0.1",
    "node-fetch": "^2.0.0",
    "p-memoize": "1.0.0",
    "package-engines-notifier": "1.1.0",
    "pify": "3.0.0",
    "read-pkg-up": "3.0.0",
    "rimraf": "2.6.2",
    "semver": "^5.4.1",
    "through2": "2.0.3",
    "update-json-file": "^1.1.1",
    "update-nodejs-notifier": "1.1.1",
    "update-notifier": "2.5.0",
    "vinyl-fs": "3.0.0",
    "write-json-file": "2.3.0"
  },
  "devDependencies": {
    "eslint": "^4.6.1",
    "eslint-plugin-ava": "^4.2.2",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-promise": "^3.5.0",
    "jest": "^22.1.4",
    "npx": "^10.0.1"
  },
  "engines": {
    "node": ">=8",
    "npm": ">=3"
  },
  "publishConfig": {
    "access": "public"
  },
  "eslintIgnore": [
    "build",
    "coverage",
    "dist",
    "flow-typed"
  ],
  "jest": {
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "lines": 55
      }
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
